-- FrontEnd-Backend // Funções de UI

HasSpecial local = função(string)
    return (string:match("%c") ou string:match("%s") ou string:match("%p") ou tonumber(string:sub(1,1))) ~= nil
fim

local GetPath = função (Instância)
	local Obj = Instância
	sequência local = {}
	temperatura local = {}
	erro local = falso
	
	enquanto Obj ~= jogo do
		se Obj == nulo então
			erro = verdadeiro
			quebrar
		fim
		table.insert(temp, Obj.Parent == game e Obj.ClassName ou tostring(Obj))
		Obj = Obj.Parent
	fim
	
	tabela.insert(string, "game:GetService(\"" .. temp[#temp] .. "\")")
	
	para i = #temp - 1, 1, -1 faça
		table.insert(string, HasSpecial(temp[i]) e "[\"" .. temp[i] .. "\"]" ou "." .. temp[i])
	fim

	return (erro e "nil - Caminho continha instância inválida" ou table.concat(string, ""))
fim

local GetType = função (Instância)
	Tipos locais =
	{
		EnumItem = função()
			retorne "Enum." .. tostring(Instance.EnumType) .. "." ..tostring(Instância.Nome)
		fim,
		Instância = função()
			retornar GetPath(Instância)
		fim,
		CFrame = função()
			retornar "CFrame.new(" .. tostring(Instância) .. ")"
		fim,
		Vetor3 = função()
			retornar "Vector3.new(" .. tostring(Instância) .. ")"
		fim,
		BrickColor = função()
			retorne "BrickColor.new(\"" .. tostring(Instância) .. "\")"
		fim,
		Cor3 = função()
			retorne "Color3.new(" .. tostring(Instância) .. ")"
		fim,
		string = função()
			local S = tostring(Instância)
			return "\"" .. (encrypt_string e S:gsub(".", function(c) return "\\" .. c:byte() end) ou S) .. "\""
		fim,
		Raio = função()
			retorne "Ray.new(Vector3.new(" .. tostring(Instance.Origin) .. "), Vector3.new(" .. tostring(Instance.Direction) .. "))"
		fim
	}

	return Types[typeof(Instance)] ~= nil e Types[typeof(Instance)]() ou tostring(Instance)
fim

tamanho_frame local = função (quadro, UDim)
	frame:TweenSize(UDim, "Out", "Quint", 0,3)
fim

local pos_frame = função(quadro, UDim)
	quadro:TweenPosition(UDim, "Out", "Quint", 0,3)
fim

tamanho local_pos_frame = função (quadro, UDim, UDim2)
	frame:TweenSizeAndPosition(UDim, UDim2, "Out", "Quint", 0,3)
fim

ocultar local = função()
	size_frame(BG, UDim2.new(0, 300, 0, 20))
	pos_frame(Título, UDim2.new(0, 0, 0, 0))
	pos_frame(Remotos, UDim2.new(0, 10, 0, 100))
	pos_frame(Fonte, UDim2.new(0, 270, 0, 100))
	BG.Draggable = verdadeiro
	SetRemotes.Visible = falso
	SetRemotesTab.Visible = falso
	Fonte.Visível = verdadeiro
	
	retornar "[]"
fim

exibição local = função()
	size_frame(BG, UDim2.new(1, -300, 1, -200))
	pos_frame(BG, UDim2.new(0,1, 0, 0,1, 0))
	pos_frame(Título, UDim2.new(0,5, -100, 0, 0))
	pos_frame(Remotos, UDim2.new(0, 10, 0, 80))
	pos_frame(Fonte, UDim2.new(0, 270, 0, 80))
	BG.Draggable = falso
	
	retornar "_"
fim

