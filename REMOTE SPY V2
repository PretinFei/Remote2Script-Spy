	FilterF.BorderColor3 = FilterF.TextColor3 == Color3.fromRGB(200, 60, 60) e Color3.fromRGB(100, 30, 30) ou Color3.fromRGB(30, 100, 30)
	filter_functions()
fim

Destaque local = função (string, palavras-chave)
    K local = {}
    S local = string
    Token local =
    {
        ["="] = verdadeiro,
        ["."] = verdadeiro,
        [","] = verdadeiro,
        ["("] = verdadeiro,
        [")"] = verdadeiro,
        ["["] = verdadeiro,
        ["]"] = verdadeiro,
        ["{"] = verdadeiro,
        ["}"] = verdadeiro,
        [":"] = verdadeiro,
        ["*"] = verdadeiro,
        ["/"] = verdadeiro,
        ["+"] = verdadeiro,
        ["-"] = verdadeiro,
        ["%"] = verdadeiro,
		[";"] = verdadeiro,
		["~"] = verdadeiro
    }
    para i, v em pares (palavras-chave) faça
        K[v] = verdadeiro
    fim
    S = S:gsub(".", função(c)
        se Token[c] ~= nulo então
            retornar "\32"
        outro
            retornar c
        fim
    fim)
    S = S:gsub("%S+", função(c)
        se K[c] ~= nulo então
            retornar c
        outro
            return (" "):rep(#c)
        fim
    fim)
  
    retornar S
fim

tokens locais = função (string)
    Token local =
    {
        ["="] = verdadeiro,
        ["."] = verdadeiro,
        [","] = verdadeiro,
        ["("] = verdadeiro,
        [")"] = verdadeiro,
        ["["] = verdadeiro,
        ["]"] = verdadeiro,
        ["{"] = verdadeiro,
        ["}"] = verdadeiro,
        [":"] = verdadeiro,
        ["*"] = verdadeiro,
        ["/"] = verdadeiro,
        ["+"] = verdadeiro,
        ["-"] = verdadeiro,
        ["%"] = verdadeiro,
		[";"] = verdadeiro,
		["~"] = verdadeiro
    }
    locais A = ""
    string:gsub(".", função(c)
        se Token[c] ~= nulo então
            UMA = UMA..c
        elseif c == "\n" então
            UMA = UMA.. "\n"
		elseif c == "\t"então
			UMA = UMA.. "\ t"
        outro
            UMA = UMA.. "\32"
        fim
    fim)
  
    retornar A
fim

strings locais = função(string)
    destaque local = ""
    cotação local = falso
    string:gsub(".", função(c)
        se citação == falso e c == "\"" então
            citação = verdadeiro
        elseif quote == true e c == "\"" então
            citação = falso
        fim
        se citação == falso e c == "\"" então
            destaque = destaque .. "\""
        elseif c == "\n" então
            destaque = destaque .. "\n"
	elseif c == "\t"então
	    destaque = destaque .. "\t"
        elseif citação == verdadeiro então
            destaque = destaque .. c
        elseif citação == falso então
            destaque = destaque .. "\32"
        fim
    fim)
  
    retornar destaque
fim

comentários locais = função(string)
    ret local = ""
    string:gsub("[^\r\n]+", função(c)
        comunicação local = falso
        local eu = 0
        c:gsub(".", função(n)
            eu = eu + 1
            se c:sub(i, i + 1) == "--" então
                comm = verdadeiro
            fim
            se comm == verdadeiro então
                ret = ret..n
            outro
                ret = ret.. "\32"
            fim
        fim)
        ret = ret
    fim)
    
    retornar ret
fim

copy_source local = função()
	script local = ""
	cópia local
	para i, v em pares (Fonte:GetChildren()) faça
		script = script .. v.SourceText.Text .. "\n"
	fim
	se área de transferência ~= nulo então
		copiar=área de transferência.set
	elseif Sinapse ~= nulo então
		copiar = função(str)
			Sinapse:Copiar(str)
		fim
	elseif setclipboard ~= nulo então	
		copiar=setclipboard
	fim
	copiar (roteiro)
fim

onclick_fullscreen local = função()
	size_pos_frame(BG, UDim2.new(1, 0, 1, 40), UDim2.new(0, 0, 0, -40))
	BG.Draggable = falso
fim

filter_remotes locais = função (tipo)
	locais n = 0
	se tipo == "Texto" então
		para i, v em pares(SetRemotes:GetChildren()) faça
			se v.Name:lower():match(Search.Text:lower()) e string ~= "" então
				v.Visível = verdadeiro
				n = n + 1
			outro
				v.Visível = falso
			fim
			se v.Visible == verdadeiro então
				v.Posição = UDim2.new(0, 10, 0, -20 + n * 30)
			outro
				v.Posição = UDim2.new(0, 10, 0, -20 + i * 30)
			fim
		fim
	fim
fim

correção local = função(string)
	if string == "/e corrigir" então
		mostrar()
		espere (0,3)
		pos_frame(BG, UDim2.new(0,1, 0, 0,1, 0))
	fim
fim

