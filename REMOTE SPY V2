-- Remotefill recursivo // UI-Backend

Tabela_TS = função (T)
    local M = {}
    para i, v em pares (T) faça
        local I = "\n\t" .. (type(i) == "número" e "[" .. i .. "] = " ou "[\"" .. i .. "\"] = ")
        table.insert(M, I .. (type(v) == "table" e Table_TS(v) ou GetType(v)))
    fim
    
    retorne "\n{" .. tabela.concat(M, ", ") .. "\n}"
fim

preenchimento de função (base)
	para i, v em pares(base:GetChildren()) faça
		se v.ClassName:match("Remote") e v.Name ~= "CharacterSoundEvent" então
			local B = SBTN:Clone()
			
			B.Parent = SetRemotes
			B.Icon.Image = (v.ClassName == "RemoteEvent" e "rbxassetid://413369506" ou "rbxassetid://413369623")
			B.NomeRemoto.Text = v.Nome
			B.ID.Text = GetPath(v)
			B.Nome = v.Nome
			B.Posição = UDim2.new(0, 10, 0, -20 + #SetRemotes:GetChildren() * 30)
			B.MouseButton1Down:Conectar(função()
				B.Enabled.Text = B.Enabled.Text == "Ativado" e "Desativado" ou "Ativado"
				B.Enabled.TextColor3 = B.Enabled.Text == "Ativado" e Color3.fromRGB(60, 200, 60) ou Color3.fromRGB(200, 60, 60)
				B.Enabled.BorderColor3 = B.Enabled.Text == "Ativado" e Color3.fromRGB(30, 100, 30) ou Color3.fromRGB(100, 30, 30)
			fim)
		fim
		preencher (v)
	fim
fim

preencher (jogo)

-- Backend // Backend de espionagem remota

local game_meta = getrawmetatable(jogo)
local game_namecall = game_meta.__namecall
nome localcall_dump = {}
atual_rmt local = nulo
local g_caller = nulo
local f_return = nulo
local Step = game:GetService("RunService").Stepped

mwr local

se setreadonly ~= nulo então
	mwr = função()
		setreadonly(game_meta, falso)
	fim
elseif make_writeable ~= nulo então	
	mwr = função()
		make_writeable(jogo_meta)
	fim
fim

mwr()

local namecall_script = função(objeto, método, ...)
	local script = "-- Script gerado por R2Sv2\n-- R2Sv2 desenvolvido por Luckyxero\n\32\n"
	argumentos locais = {}
	para i, v em pares{...} faça
		script = script .. "local A_" .. i .. " = " .. (type(v) == "table" e Table_TS(v) ou GetType(v)) .. "\n"
		tabela.inserir(args, "A_" ..i)
	fim
	script = script .. "local Event = " .. GetPath(objeto) .. "\n\n"
	script = script .. "Evento:" .. método .. "(" .. table.concat(args, ", ") .. ")"
	
	script de retorno
fim

dump_script local = função(script)
	Fonte:ClearAllChildren()
	linhas locais = 0
	script:gsub("[^\r\n]+", função(c)
		linhas = linhas + 1
		guias locais = 0
		c:gsub("%\t", function() guias = guias + 1 final)
		linha local = ScriptLine:Clone()
		linha.Parent = Fonte
		linha.SourceText.Text = c
		linha.Linha.Text = linhas
		line.RemoteHighlight.Text = Destaque(c, {"FireServer", "InvokeServer", "invokeServer", "fireServer"})
		line.Position = UDim2.new(0, tabs * (17 * 2), 0, -17 + #Fonte:GetChildren() * 17)
		line.Globals.Text = Destaque(c, global_env)
		line.Line.Position = UDim2.new(0, 0 - guias * (17 * 2), 0, 0)
		linha.Strings.Text = strings(c)
		line.Keywords.Text = Destaque(c, lua_keywords)
		linha.Tokens.Text = Tokens(c)
		linha.Comments.Text = comentários(c)
	fim)
fim

log_remote local = função (tabela)
	se SetRemotes[table.object.Name].Enabled.Text == "Disabled" então retorne end
	local B = RBTN:Clone()
	g_caller = tabela.caller
	disparo_remoto = disparo_remoto + 1
	Total.Text = remotes_fired

	B.Parent = Remotos
	B.Posição = UDim2.new(0, 10, 0, -20 + #Remotes:GetChildren() * 30)
	B.Icon.Image = table.method == "FireServer" e "rbxassetid://413369506" ou "rbxassetid://413369623"
	B.NomeRemoto.Text = tabela.objeto.Nome
	B.ID.Text = tostring(remotes_fired)
	B.MouseButton1Down:Conectar(função()
		dump_script(tabela.script)
		g_caller = tabela.caller
		f_return = table.freturn == nulo e table.object.Name .. "não é RemoteFunction" ou table.freturn
	fim)
fim


local get_namecall_dump = função(script, objeto, ...)
	local Ret = nulo
	se object.ClassName == "RemoteFunction" então
		freturn local = {pcall(object.InvokeServer, object, ...)}
		freturn = {select(2, descompactar(freturn))}
		
		se #freturn == 0 então
			Ret = object.Name .. "é um tipo vazio RemoteFunction."
		outro
			Ret = Tabela_TS(freturn)
		fim
	fim
	nomecall_dump[#nomecall_dump + 1] =
	{	
		script = namecall_script(object, object.ClassName == "RemoteEvent" e "FireServer" ou "InvokeServer", ...),
		chamador = roteiro,
		objeto = objeto,
		método = object.ClassName == "RemoteEvent" e "FireServer" ou "InvokeServer",
		freturn = Ret
	}
fim

GetReturn.MouseButton1Down:Conectar(função()
	dump_script(f_return)
fim)

Decompile.MouseButton1Down:Conectar(função()
	dump_script('Descompilação atualmente quebrada com LuaU.')
fim)

Etapa:Conectar(função()
	enquanto #namecall_dump > 0 faça
		log_remote(tabela.remove(namecall_dump, 1))
	fim
fim)

local on_namecall = função(objeto, ...)
	método local = tostring(getnamecallmethod())
	argumentos locais = {...}
	se object.Name ~= "CharacterSoundEvent" e método:match("Server") e spy_enabled == true então get_namecall_dump(getfenv(2).script, object, unpack(args)) end

	retornar game_namecall(objeto, ...)
fim

game_meta.__namecall=on_namecall
