		v.Visible = não (FilterF.TextColor3 == Color3.fromRGB(60, 200, 60) e v.Icon.Image == "rbxassetid://413369506")
		se v.Visible == verdadeiro então
			n = n + 1
			v.Posição = UDim2.new(0, 10, 0, -20 + n * 30)
		outro
			v.Posição = UDim2.new(0, 10, 0, -20 + i * 30)
		fim
	fim
fim

