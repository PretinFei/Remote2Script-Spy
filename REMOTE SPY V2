	FilterE.TextColor3 = FilterE.TextColor3 == Color3.fromRGB(60, 200, 60) e Color3.fromRGB(200, 60, 60) ou Color3.fromRGB(60, 200, 60)
	FilterE.BorderColor3 = FilterE.TextColor3 == Color3.fromRGB(200, 60, 60) e Color3.fromRGB(100, 30, 30) ou Color3.fromRGB(30, 100, 30)
